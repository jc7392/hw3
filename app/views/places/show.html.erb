<div class="container">
  <h1> <%= @place.name %> </h1>
  <ul>
    <% @entries.each do |entry| %>
        <h2> <%=  entry.title %> </h2>
        <p> <%= entry.posted_on %> </p>
        <p> <%= entry.description %> </p>
        <br>
    <% end %>
    <%= link_to "New Post", new_entry_path(place_id: @place.id) %>
  </ul>
</div>