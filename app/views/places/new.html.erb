<div class="container mt-3">
<h1> New Place </h1>
<%= form_with model: @place do |form| %>
  <div class="field form-group">
    <%= form.label :name %><br>
    <%= form.text_field :name %>
    <% @place.errors.full_messages_for(:name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div class="actions">
    <%= form.submit "Create Place", class: "btn btn-primary" %>
  </div>
<% end %>